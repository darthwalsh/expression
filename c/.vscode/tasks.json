{
    "version": "0.1.0",
    "command": "gcc",
    "args": ["-Wall", "*.c", "-g", "-o", "test"],
    "problemMatcher": {
        "owner": "c",
        "fileLocation": "absolute",
        "pattern": {
            "regexp": "^(.*):(\\d+):\\s+(.*)",
            "file": 1,
            "line": 2,
            "message": 3
        }
    }
}